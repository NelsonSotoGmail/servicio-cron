{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "strict": true,
    "allowJs": false,
    "noEmit": false
  },
  "include": [
    "./cron-runner.ts",
    "./cron/**/*.ts"
  ]
}


